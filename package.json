{
  "name": "kan-colle-widget",
  "version": "3.0.0-alpha.59",
  "description": "『艦これ』をちょっとだけ快適にするやつ",
  "engines": {
    "node": "~12.14.0",
    "npm": "6.13.4"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack --progress --watch",
    "version": "node ./scripts/version.js",
    "build": "webpack && rm dest/css/*.js",
    "publish": "node ./scripts/publish.js ./release/kcwidget.zip",
    "announce": "node ./scripts/announce.js",
    "tweet": "node ./scripts/tweet.js",
    "pack": "./scripts/pack.sh",
    "lint": "eslint ./src/js/**/*.tsx ./src/js/**/*.ts ./tests/**/*.ts",
    "test": "jest --coverage"
  },
  "repository": "git+https://github.com/otiai10/kanColleWidget.git",
  "author": "otiai10",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/otiai10/kanColleWidget/issues"
  },
  "homepage": "https://github.com/otiai10/kanColleWidget#readme",
  "devDependencies": {
    "@types/chrome": "0.0.114",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.0.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-cli": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "request-promise": "^4.2.2",
    "sass-loader": "^8.0.2",
    "sinon-chrome": "^3.0.1",
    "source-map-loader": "^1.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "twitter": "^1.7.1",
    "typescript": "^3.9.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/js/*.ts",
      "src/js/Applications/**/*.ts",
      "src/js/Services/**/*.ts"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup/chrome.ts",
      "<rootDir>/tests/setup/image-onload.ts",
      "jest-canvas-mock"
    ],
    "testRegex": "tests/.*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/v2_backup/"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "chomex": "^2.0.2",
    "classnames": "^2.2.6",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "spectre.css": "^0.5.3"
  }
}
